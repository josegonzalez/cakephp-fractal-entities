{
    "name": "josegonzalez/cakephp-fractal-entities",
    "description": "CakePHP View-layer wrapper for the Fractal library",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "api", "json", "rest"],
    "homepage": "http://github.com/josegonzalez/cakephp-fractal-entities",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Diaz-Gonzalez",
            "email": "cakephp+fractal-entities@josediazgonzalez.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "cakephp/cakephp": "~3.1",
        "league/fractal": "^0.13.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.33",
        "friendsofcake/cakephp-test-utilities": "dev-master",
        "cakephp/cakephp-codesniffer": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "FractalEntities\\": "src",
            "FractalEntities\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FractalEntities\\Test\\": "tests"
        }
    }
}
